{
    "name": "GitHub Gantt Chart",
    "description": "A Gantt-chart visualization of GitHub issues for repository.",
    "version": "0.1",
    "manifest_version": 2,
    "app": {
	"background": {
	    "scripts": ["background.js"]
	}
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
	"https://api.github.com/*",
        "storage"
    ]
}
